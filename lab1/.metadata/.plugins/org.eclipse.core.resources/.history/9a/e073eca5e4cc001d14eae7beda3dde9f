package lab1;

import java.util.ArrayList;

public class DetailShapeService {
	public static String GetUniqueShape(ArrayList<Detail> details)
	{
		String result = "Деталей с уникальной формой не найдено.";
		boolean isUnique = true;
		
		for (int i = 0; i < details.size(); i++)
		{
			for (int j = i + 1; j < details.size(); j++)
			{
				if (details.get(i).shape == details.get(j).shape)
				{
					isUnique = false;
				}
			}
		}
	}
}
